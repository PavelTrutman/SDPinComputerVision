\gdef\fltvz@setfont{%
  \renewcommand{\rmdefault}{fcmr}
  \renewcommand{\sfdefault}{fcmss}
  \renewcommand{\ttdefault}{fcmtt}

  \SetSymbolFont{operators}   {normal}{OT1}{fcmr} {m}{n}
  \SetSymbolFont{letters}     {normal}{OML}{fcmm} {m}{it}
  \SetSymbolFont{symbols}     {normal}{OMS}{fcmsy}{m}{n}
  \SetSymbolFont{largesymbols}{normal}{OMX}{fcmex}{m}{n}
  \SetSymbolFont{operators}   {bold}  {OT1}{fcmr} {bx}{n}
  \SetSymbolFont{letters}     {bold}  {OML}{fcmm} {b}{it}
  \SetSymbolFont{symbols}     {bold}  {OMS}{fcmsy}{b}{n}
  \SetSymbolFont{largesymbols}{bold}  {OMX}{fcmex}{m}{n}

  \SetMathAlphabet{\mathbf}{normal}{OT1}{fcmr}{bx}{n}
  \SetMathAlphabet{\mathsf}{normal}{OT1}{fcmss}{m}{n}
  \SetMathAlphabet{\mathit}{normal}{OT1}{fcmr}{m}{it}
  \SetMathAlphabet{\mathtt}{normal}{OT1}{fcmtt}{m}{n}
  \SetMathAlphabet{\mathbf}{bold}  {OT1}{fcmr}{bx}{n}
  \SetMathAlphabet{\mathsf}{bold}  {OT1}{fcmss}{bx}{n}
  \SetMathAlphabet{\mathit}{bold}  {OT1}{fcmr}{bx}{it}
  \SetMathAlphabet{\mathtt}{bold}  {OT1}{fcmtt}{m}{n}
}

